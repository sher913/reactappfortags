
#Curl command for all datasets

curl "http://localhost:8080/datasets?q=search&input=*" -X GET -H 'X-RestLi-Protocol-Version: 2.0.0' -H 'X-RestLi-Method: finder' | jq



#First element in json
curl "http://localhost:8080/datasets?q=search&input=*" -X GET -H 'X-RestLi-Protocol-Version: 2.0.0' -H 'X-RestLi-Method: finder' | jq '.["elements"][0]'

#js version
var url = "http://localhost:8080/datasets?q=search&input=*";

var xhr = new XMLHttpRequest();
xhr.open("GET", url);

xhr.setRequestHeader("X-RestLi-Protocol-Version", "2.0.0");
xhr.setRequestHeader("X-RestLi-Method", "finder");

xhr.onreadystatechange = function () {
   if (xhr.readyState === 4) {
      console.log(xhr.status);
      console.log(xhr.responseText);
   }};

xhr.send();